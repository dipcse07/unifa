# platform :ios, '9.0'

target 'Unifa' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Pods for Unifa
  #Rx Swift
   pod 'RxSwift', '6.6.0'
   pod 'RxCocoa', '6.6.0'
   pod 'RxDataSources', '~> 5.0'
   pod 'NSObject+Rx'
  
  # Network layer
   pod 'Alamofire', '~> 5.8'
   

  # Image Caching
   pod 'Kingfisher', '~> 7.10'
   

  target 'UnifaTests' do
    inherit! :search_paths
    # Pods for testing
    pod 'RxTest'
  end

  target 'UnifaUITests' do
    # Pods for testing
  end

end
